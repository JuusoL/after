    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.38",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "@types/es6-shim": "0.0.31",
    "compression-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "2.1.6",
    "css-loader": "0.25.0",
    "dedupe-plugin": "1.0.0",
    "html-loader": "0.4.3",
    "node-static": "0.7.8",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.4",
    "ts-loader": "0.8.2",
